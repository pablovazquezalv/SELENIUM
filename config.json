{
    "sites": [
        
     
        {
            "name": "frikko",
            "url": "https://www.frikko.com",
            "actions": [
                {
                    "type": "click",
                    "selector": {
                        "by": "CSS_SELECTOR",
                        "value": ".menu-item-text"
                    }
                },
                {
                    "type": "send_keys",
                    "selector": {
                        "by": "CSS_SELECTOR",
                        "value": ".facetwp-search"
                    },
                    "value": "minisplit"
                },
                {
                    "type": "wait_for_element",
                    "selector": {
                        "by": "CSS_SELECTOR",
                        "value": ".woopack-grid-wrap"
                    }
                },
                {
                    "type": "scroll",
                    "settings": {
                        "repetitions": 2,
                        "wait_time": 2
                    }
                },
                {
                    "type": "extract",
                    "container_selector": {
                        "by": "CSS_SELECTOR",
                        "value": ".woopack-grid-wrap"
                    },
                    "item_selector": {
                        "by": "ID",
                        "value": "woopack-product-3966"
                    },
                    "fields": [
                        {
                            "name": "name",
                            "selector": {
                                "by": "CSS_SELECTOR",
                                "value": "h3.woopack-product-title"
                            }
                        },
                        {
                            "name": "price",
                            "selector": {
                                "by": "CSS_SELECTOR",
                                "value": "span.price"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}